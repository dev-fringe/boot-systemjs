{
  "name": "prototype-web-boot",
  "version": "0.0.1",
  "dependencies": {
    "jspm": "^0.15.7",
    "systemjs-builder": "^0.10.6"
  },
  "scripts": {
    "jspm-install": "jspm install"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "src/main/webapp",
      "packages": "src/main/webapp/lib"
    },
    "dependencies": {
      "css": "github:systemjs/plugin-css@^0.1.23",
      "davatron5000/FitText.js": "github:davatron5000/FitText.js@^1.2.0",
      "dimsemenov/Magnific-Popup": "github:dimsemenov/Magnific-Popup@^1.1.0",
      "joequery/Stupid-Table-Plugin": "github:joequery/Stupid-Table-Plugin@^1.0.1",
      "jquery": "github:components/jquery@^1.0.0",
      "xpepermint/history-events": "github:xpepermint/history-events@^1.0.4"
    },
    "devDependencies": {
      "traceur": "github:jmcriffey/bower-traceur@0.0.88",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.88"
    },
    "overrides": {
      "github:joequery/Stupid-Table-Plugin@1.0.1": {
        "shim": {
          "*": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "github:components/jquery"
        },
        "format": "global"
      },
      "github:davatron5000/FitText.js@1.2.0": {
        "shim": {
          "*": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "github:components/jquery"
        },
        "format": "global"
      },
      "github:cowboy/jquery-hashchange@master": {
        "shim": {
          "*": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "github:components/jquery"
        },
        "format": "global"
      },
      "github:dimsemenov/Magnific-Popup@1.1.0": {
        "shim": {
          "*": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "github:components/jquery"
        },
        "format": "global"
      }
    }
  }
}
